{
  "manifest_version": 3,
  "name": "Discord Link Redirector",
  "version": "1.0",
  "description": "DiscordリンクをDesktop Appで開きます",
  "permissions": ["tabs", "scripting", "webNavigation"],
  "host_permissions": ["*://discord.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}